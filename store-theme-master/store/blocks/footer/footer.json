  "image#formas-de-parceiro-footer": {
    "props": {
      "src": "/arquivos/parceiro-footer-vtex-io.png",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "storePrint"
    }
  },



  "flex-layout.col#seguranca":{
    "children": ["rich-text#formas-de-seguranca-title","image#formas-de-seguranca-footer"],
    "props": {
      "blockClass": "formas-de-pagemanto-footer"
    }

  },

  "rich-text#formas-de-seguranca-title": {
    "props": {
      "text": "Segurança",
      "blockClass": "formas-de-pagamento-title"
    }
  },
  "image#formas-de-seguranca-footer": {
    "props": {
      "src": "/arquivos/pci.png",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "storePrint"
    }
  },












  "accepted-payment-methods": {
    "props": {
      "paymentMethods": ["MasterCard", "Visa", "Diners Club"]
    }
  },




  "flex-layout.row#footer-3-desktop": {
