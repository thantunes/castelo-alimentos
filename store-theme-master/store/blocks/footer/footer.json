  },


  "footer-layout.mobile": {
    "children": [
    
      "newsletter",
      "flex-layout.row#logo-footer",
      "flex-layout.row#fale-conosco",
      "flex-layout.row#1-footer-mobile",
      "flex-layout.row#formas-de-pagemanto",

