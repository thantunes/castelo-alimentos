{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "children": [
    
      "newsletter",
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer-2-desktop",
      "flex-layout.row#footer-3-desktop"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "flex-layout.col#logo-footer",
      "flex-layout.col#fale-conosco",
      "flex-layout.col#institucional",
      "flex-layout.col#area-do-cliente",
    
      "flex-layout.col#funcionamento-loja-de-fabrica"
    ],
    "props": {
      "blockClass": "menu-row",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },







  

"newsletter":{

  "props": {
    "label":"Fique por dentro de nossas novidades e promoções em primeira mão",
    "placeholder":"Digite aqui seu e-mail",
    "submit":"Enviar"
  }
},
  

"flex-layout.row#logo-footer":{

  "children": ["flex-layout.col#logo-footer"],
  "props": {
    "blockClass": "logo-mobile-footer"
  }
},


  "flex-layout.col#logo-footer": {
    "props": {
      "verticalAlign": "top"
    },
    "children": ["logo#footer","flex-layout.col#redes-sociais-footer"]
  },

  "logo#footer": {
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "/arquivos/logo-castelo-footer-io.png",
      "width": "180",
      "height": "200"
    }
  },




  "flex-layout.col#redes-sociais-footer": {
    "props": {
      "verticalAlign": "center",
      "blockClass": "redes-sociais-footer"
    },
    "children": [
      "image#facebook",
      "image#instagram",
      "image#youtube",
      "image#linkedin",
      "image#tiktok"


    ]
  },

  "image#facebook": {
    "props": {
      "src": "/arquivos/facebook-icon.png",
      "blockClass":"face"
     
    }
  },

  "image#instagram": {
    "props": {
      "src": "/arquivos/instagram-icon.png",
      "blockClass":"instagram"
    }
  },

  "image#youtube": {
    "props": {
      "src": "/arquivos/youtube-icon.png",
      "blockClass":"youtube"
    }
  },

  "image#linkedin": {
    "props": {
      "src": "/arquivos/linkedin-icon.png",
      "blockClass":"linkedin"
    }
  },

  "image#tiktok": {
    "props": {
      "src": "/arquivos/tiktok-icon.png",
      "blockClass":"tiktok"
    }
  },




  "social-networks": {
    "props": {
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/vtexonline/"
        },
        {
          "name": "Instagram",
          "url": "https://www.instagram.com/vtextruecloud/"
        },
        {
          "name": "Twitter",
          "url": "https://twitter.com/vtexonline"
        },
        {
          "name": "Youtube",
          "url": "https://www.youtube.com/user/VTEXTV"
        }
      ]
    }
  },




  "flex-layout.col#institucional": {
    "props": {
      "verticalAlign": "top"
    },
    "children": [
      "rich-text#institucional-footer-title",
      "vtex.menu@2.x:menu#Products"
    
    ]
  },

 

  "rich-text#institucional-footer-title": {
    "props": {
      "text": "Institucional",
      "blockClass": "footer-fale-conosco"
    }
  },


  "flex-layout.row#fale-conosco":{

    "children": [ "flex-layout.col#fale-conosco"],
    "props": {
      "blockClass": "footer-fale-conosco-mobile"
    }
  },


  "flex-layout.col#fale-conosco": {
    "props": {
      "verticalAlign": "top"
    },
    "children": [
      "rich-text#faleconosco-footer-title",
      "rich-text#atendimento",
      "rich-text#telefone",
      "rich-text#email"
    
    ]
  },

  "rich-text#faleconosco-footer-title": {
    "props": {
      "text": "Fale Conosco",
      "blockClass": "footer-fale-conosco"
    }
  },






  
  "flex-layout.col#area-do-cliente": {
    "props": {
      "verticalAlign": "top"
    },
    "children": [
      "rich-text#area-do-cliente-footer-title",
      "vtex.menu@2.x:menu#footer-clothing"
    
    ]
  },


  "rich-text#area-do-cliente-footer-title": {
    "props": {

      "text": "Área do cliente",
      "blockClass": "footer-area-do-cliente-title"
 
    }
  },


  "rich-text#atendimento": {
    "props": {

      "text": "Segunda à Quinta-feira 9h às 12h / 13 às 18h \n e Sexta-feira 9h às 12h / 13h às 17h",
      "blockClass": "footer-horario-de-atendimento"
    }
  },

  "rich-text#telefone": {
    "props": {
      "text": "0800 771 6111",
      "blockClass": "footer-telefone-atendimento"
    }
  },


  "rich-text#email": {
    "props": {
      "text": "lojavirtual@casteloalimentos.com.br",
      "blockClass": "footer-email-atendimento"
    }
  },


  "flex-layout.col#funcionamento-loja-de-fabrica": {
    "props": {
      "verticalAlign": "top"
    },
    "children": [
      "rich-text#funcionamento-loja-de-fabrica-footer-title",
      "rich-text#funcionamento-loja-fabrica-seg-sex",
      "rich-text#funcionamento-loja-fabrica-sabado",
      "rich-text#funcionamento-loja-fabrica-fone",
      "rich-text#funcionamento-loja-fabrica-end"
     
    
    ]
  },


  "rich-text#funcionamento-loja-de-fabrica-footer-title": {
    "props": {
      "text": "Funcionamento Loja de Fábrica",
      "blockClass": "funcionamento-loja-de-fabrica"
    }
  },



  "rich-text#funcionamento-loja-fabrica-seg-sex": {
    "props": {
      "text": "Segunda à sexta, das 8h30 às 17h45",
      "blockClass": "funcionamento-loja-de-fabrica-text"
    }
  },




  "rich-text#funcionamento-loja-fabrica-sabado": {
    "props": {
      "text": "Sábado, das 8h30 às 15h45",
      "blockClass": "funcionamento-loja-de-fabrica-text"
    }
  },






  "rich-text#funcionamento-loja-fabrica-fone": {
    "props": {
      "text": "Fone: (11) 4589-7710",
      "blockClass": "funcionamento-loja-de-fabrica-text"
    }
  },


  "rich-text#funcionamento-loja-fabrica-end": {
    "props": {
      "text": "Av. Prof.ª Maria do Carmo Guimarães \n Pellegrini, 480 - Retiro, Jundiaí - SP",
      "blockClass": "funcionamento-loja-de-fabrica-text"
    }
  },



  "flex-layout.row#footer-2-desktop": {
    "children": ["flex-layout.col#formas-de-pagemanto","flex-layout.col#parceiro","flex-layout.col#seguranca"],
    "props": {
      "blockClass": "payment-methods"
    }
  },


  "flex-layout.row#formas-de-pagemanto":{
    "children": [ "flex-layout.col#formas-de-pagemanto"],
    "props": {
      "blockClass": "formas-de-pagamento-mobila"
    }

  },


  "flex-layout.col#formas-de-pagemanto":{
    "children": ["rich-text#formas-de-pagamento-title","image#formas-de-pagamento-footer"],
    "props": {
      "blockClass": "formas-de-pagemanto-footer"
    }

  },

