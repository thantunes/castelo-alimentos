    }
  },
  "image#formas-de-pagamento-footer": {
    "props": {
      "src": "/arquivos/formas-de-pagamento-castelo-io.png",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "storePrint"
    }
  },


  "flex-layout.row#seguranca-parceiro-mobile":{
    "children": ["flex-layout.col#parceiro","flex-layout.col#seguranca"],
    "props": {
      "blockClass": "parceiroseguranca-footer-mobile"
    }
  },


