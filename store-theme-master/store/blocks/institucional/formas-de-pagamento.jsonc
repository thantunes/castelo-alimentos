    "flex-layout.row#formasdepagamentoContent":{
      "props":{
        "blockClass":"InstitucionalContent",
        "preventHorizontalStretch":true
      },
      "children":[
        "flex-layout.col#formasdepagamentoLinks-container",
        "rich-text#formasdepagamentoContent"
      ]
    },
    "flex-layout.col#formasdepagamentoLinks-container":{
      "props":{
        "blockClass":"intitucionalLinks-container",
        "preventHorizontalStretch":true,
        "preventVerticalStretch":true
